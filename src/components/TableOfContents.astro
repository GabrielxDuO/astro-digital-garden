---
// https://github.com/withastro/starlight/blob/main/packages/starlight/components/TableOfContents.astro
import type { Props } from "@astrojs/starlight/props";
import Default from "@astrojs/starlight/components/TableOfContents.astro";
import Backlinks from "./Backlinks.astro";
---

<Default {...Astro.props}>
  <slot />
</Default>

{
  Astro.props.entry.data.backlinks !== false && (
    <Backlinks entry={Astro.props.entry as any} />
  )
}
